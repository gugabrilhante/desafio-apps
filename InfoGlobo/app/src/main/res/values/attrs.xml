<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="InfoGloboTextView">
        <attr name="dinFamily"/>
        <attr name="customTextSize"/>
    </declare-styleable>


    <attr name="dinFamily" format="enum">
        <enum name="regular" value="0"/>
        <enum name="medium" value="1"/>
        <enum name="light" value="2"/>
        <enum name="black" value="3"/>
        <enum name="bold" value="4"/>
    </attr>

    <attr name="customTextSize" format="dimension"/>


</resources>